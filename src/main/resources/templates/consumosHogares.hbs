{{#partial "contenido"}}


 	{{#if admin}}
		{{> adminNavbar.hbs }}
	{{else}}
		{{#if cliente}}
			{{> clienteNavBar.hbs }}
		{{/if}}
	{{/if}}

    <h1 class="text-center font-weight-bold mb-5">CONSUMO POR HOGAR</h1>       

    <div class="container">       

      <table class="table table-striped table-bordered text-center">
        <thead class="thead-dark">
          <tr>            
            <th scope="col">CLIENTE</th>
            <th scope="col">CONSUMO</th>          
          </tr>
        </thead>
        <tbody>
          {{#each consumosClientes}}
          <tr>                                    
            <td>{{@key}}</td>
            <td>{{this}}</td>
          </tr>  
          {{/each}}       
        </tbody>
      </table>

    </div>

{{/partial}}

{{>layout.hbs}}
